generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organization {
  id          String   @id @default(cuid())
  clerk_id    String?  @unique
  slug        String?   @unique
  name        String?
  image       String?
}

model User {
  id            String    @id @default(cuid())
  clerk_id      String?   @unique
  email         String?   @unique
  first_name    String?
  last_name     String?
  image         String?
}


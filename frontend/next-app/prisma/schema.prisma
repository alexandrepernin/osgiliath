generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Home {
  id          String   @id @default(cuid())
  description String
  image       String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model User {
  id            String    @id @default(cuid())
  clerk_id      String?   @unique
  first_name    String?
  last_name     String?
  email         String?   @unique
  emailVerified DateTime?
  password      String?
  image         String?
}

